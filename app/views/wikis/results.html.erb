<h1>These are the results</h1>

<br><br>
<div class="row">
  <div class="col-md-1"></div>
  <div class="col-md-10">
    <% @articles.each do |article| %>
    <div class="panel-primary">
      <div class="panel-heading">
        <a class="text-muted" href="/wikis/<%=@wiki.id%>/articles/<%=article.id%>"><%=article.title%></a>
      </div>
      <div class="panel-content"><%=article.content%></div>
      </br>
    </div>
    <% end %>
  </div>
  <div class="col-md-1"></div>
</div>
